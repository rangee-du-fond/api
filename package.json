  "bugs": {
    "url": "https://github.com/rangee-du-fond/api/issues"
  },
  "homepage": "https://github.com/rangee-du-fond/api#readme",
  "devDependencies": {
    "@types/hapi__hapi": "^19.0.0",
    "@types/hapi__inert": "^5.2.0",
    "@types/hapi__vision": "^5.5.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.9.1",
    "@types/pg": "^7.14.3",
    "dotenv-cli": "^3.1.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@hapi/hapi": "^19.0.5",
